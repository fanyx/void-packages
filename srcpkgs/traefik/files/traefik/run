#!/bin/sh
[ -r ./conf ] && . ./conf
exec 2>&1

exec setpriv --reuid _traefik --regid _traefik --clear-groups \
	--ambient-caps -all,+net_bind_service \
	--inh-caps -all,+net_bind_service \
	--bounding-set -all,+net_bind_service \
	--no-new-privs -- traefik ${OPTS:---configFile=/etc/traefik/traefik.yml}
